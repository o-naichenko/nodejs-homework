{
  "name": "template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon node --trace-warnings ./bin/server.js",
    "lint": "eslint **/*.js",
    "lint:fix": "eslint --fix **/*.js",
    "dev:debug": "node --inspect ./bin/server.js",
    "test": "cross-env NODE_ENV=test jest --no-cache --verbose --detectOpenHandles",
    "test:coverage": "jest --coverage --passWithNoTests"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.4",
    "bcryptjs": "2.4.3",
    "cloudinary": "1.25.1",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "5.2.6",
    "gravatar": "1.8.1",
    "jest": "^27.0.0",
    "jimp": "0.16.1",
    "joi": "17.4.0",
    "joi-phone-number": "^5.0.1",
    "jsonwebtoken": "8.5.1",
    "mailgen": "^2.0.14",
    "mongodb": "3.6.4",
    "mongoose": "5.12.0",
    "mongoose-paginate-v2": "1.3.18",
    "multer": "1.4.2",
    "nanoid": "^3.1.23",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "supertest": "^6.1.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "cors": "^2.8.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "morgan": "^1.10.0",
    "nodemon": "2.0.7"
  }
